{
  "name": "terraform-module-aws-vpc",
  "description": "Terraform AWS VPC Module",
  "version": "0.1.0",
  "repositoryUrl": "https://gitlab.localrealm.net/automation/terraform/terraform-modules/terraform-module-aws-vpc.git",
  "repository": {
    "type": "git",
    "url": "https://gitlab.localrealm.net/automation/terraform/terraform-modules/terraform-module-aws-vpc.git"
  },
  "author": "Paul Canham",
  "release": {
    "tagFormat": "${version}",
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "feat",
              "release": "patch"
            },
            {
              "type": "hotfix",
              "release": "patch"
            },
            {
              "type": "patch",
              "release": "patch"
            },
            {
              "type": "minor",
              "release": "minor"
            },
            {
              "type": "breaking",
              "release": "major"
            },
            {
              "type": "test_build",
              "release": "patch"
            },
            {
              "scope": "no-release",
              "release": false
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          },
          "writerOpts": {
            "commitsSort": [
              "subject",
              "scope"
            ]
          }
        }
      ],
      "@semantic-release/changelog",
      [
        "@semantic-release/git",
        {
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^13.0.0",
    "semantic-release": "^25.0.0",
    "semantic-release-replace-plugin": "^1.2.7",
    "@commitlint/config-conventional": "^20.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/release-notes-generator": "^14.0.0",
    "@semantic-release/git": "^10.0.0"
  }
}
